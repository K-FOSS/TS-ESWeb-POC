{
  "name": "ts-esweb-poc",
  "version": "0.0.1",
  "description": "Proof of concept example for modern web development",
  "main": "src/index.ts",
  "type": "module",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "runner": "node --loader @k-foss/ts-esnode --experimental-specifier-resolution=node --experimental-import-meta-resolve --harmony-top-level-await",
    "postinstall": "node --loader @k-foss/ts-esnode --experimental-specifier-resolution=node --experimental-import-meta-resolve --harmony-top-level-await ./bin/postInstall.ts && patch-package",
    "build": "npm run runner -- ./bin/build.ts",
    "prettier": "prettier --config .prettierrc --check 'src/**/*ts' 'bin/**/*.ts'",
    "lint": "eslint --ext tsx,ts src",
    "test": "ts-estest ./src",
    "start": "node --loader @k-foss/ts-esnode --experimental-specifier-resolution=node --experimental-import-meta-resolve --harmony-top-level-await ./src/Server/index.ts",
    "dev": "nodemon"
  },
  "dependencies": {
    "@k-foss/ts-worker": "^1.0.5",
    "@wessberg/cjs-to-esm-transformer": "0.0.22",
    "apollo-server-fastify": "^2.17.0",
    "class-validator": "^0.12.2",
    "cross-fetch": "^3.0.5",
    "fastify": "^3.4.1",
    "fastify-websocket": "^2.0.7",
    "gqless": "0.0.1-alpha.29",
    "graphql": "^15.3.0",
    "gunzip-maybe": "^1.4.2",
    "history": "^5.0.0",
    "patch-package": "^6.2.2",
    "react": "0.0.0-experimental-94c0244ba",
    "react-dom": "0.0.0-experimental-94c0244ba",
    "react-is": "^16.13.1",
    "react-refresh": "^0.8.3",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.0.0",
    "typescript": "^4.0.3",
    "workbox-window": "^5.1.3"
  },
  "devDependencies": {
    "@apollo/client": "^3.2.0",
    "@gqless/cli": "0.0.1-alpha.31",
    "@k-foss/ts-esnode": "^1.7.0",
    "@k-foss/ts-estests": "^1.0.4",
    "@types/history": "^4.7.6",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/workbox-sw": "^4.3.1",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.28.1",
    "apollo-server-testing": "^2.17.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2"
  },
  "license": "MIT"
}
